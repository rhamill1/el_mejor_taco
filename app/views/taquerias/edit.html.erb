<%= form_for :taqueria, url: "/taquerias", method: "update" do |f| %>
  <%= f.text_field :taqueria_name %><br>
  <%= f.text_field :title %><br>
  <%= f.text_area :description %><br>
  <%= f.text_field :location %><br>
  <%= f.text_field :primary_img %><br>
  <%= f.text_field :secondary_img %><br>
  <%= f.text_field :region_id %><br>
  <%= f.submit "Save Taqueria" %>
<% end %>
